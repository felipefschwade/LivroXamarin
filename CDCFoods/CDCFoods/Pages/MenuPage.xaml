<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CDCFoods.Pages.MenuPage"
             Title="Menu de Opções">
    <ContentPage.Content>
        <StackLayout VerticalOptions="Center">
            <Button Text="Garçons" Image="icone_garcons.png" Clicked="Garcons_Clicked"/>
            <Button Text="Entregadores" Image="icone_entregadores.png" Clicked="Entregadores_Clicked" />
            <Button Text="Tipos de Itens" Image="icone_tipositenscardapio.png" Clicked="TiposItensCardapio_Clicked"/>
            <Button Text="Cardápio" Image="icone_tipositenscardapio.png" Clicked="Cardapio_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>